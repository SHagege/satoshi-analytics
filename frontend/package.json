{
  "name": "satoshi-analytics",
  "version": "0.1.0",
  "homepage": "https://shagege.github.io/satoshi-analytics",
  "private": true,
  "dependencies": {
    "axios": "^0.19.2",
    "gh-pages": "^2.1.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "css": "node-sass src/sass/mystyles.scss -o public",
    "css:watch": "npm run css && node-sass src/sass/mystyles.scss -wo public",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "bulma": "^0.8.0",
    "node-sass": "^4.13.1"
  }
}
